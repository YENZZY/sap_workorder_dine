{
  "_version": "1.59.0",
  "sap.app": {
    "id": "dinewkorder",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.14.4",
      "toolsId": "2991163f-3bd6-4350-8627-71af427517d7"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZSBPP_PRODORDER",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      },
      "mainService2": {
        "uri": "/sap/opu/odata/sap/ZSBPP_SALESORDERITEM",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "fullWidth": true,
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.124.5",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "dinewkorder.i18n.i18n"
        }
      },
      "mainData": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      },
      "dineData": {
        "dataSource": "mainService2",
        "preload": true,
        "settings": {}
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "dinewkorder.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "Main",
          "pattern": ":?query:",
          "target": [
            "Main"
          ]
        },
        {
          "name": "PageSO",
          "pattern": "PageSO/",
          "target": [
            "PageSO"
          ]
        },
        {
          "name": "PageMP",
          "pattern": "PageMP/",
          "target": [
            "PageMP"
          ]
        }
      ],
      "targets": {
        "Main": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Main",
          "viewName": "Main"
        },
        "PageSO": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "PageSO",
          "viewName": "PageSO"
        },
        "PageMP": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "PageMP",
          "viewName": "PageMP"
        }
      }
    },
    "rootView": {
      "viewName": "dinewkorder.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
